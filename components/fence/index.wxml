<view class="container">
  <!-- spec规格名 -->
  <view class="title">{{ fence.title }}</view>
  <view class="row-container">
    <block wx:for="{{ fence.cells }}" wx:key="index">
      <s-cell
        x="{{ x }}"
        y="{{ index }}"
        cell="{{ item }}"
        class="cell"
      ></s-cell>
    </block>
    <view class="hr"></view>
  </view>
</view>
